{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./",
    "declaration": true
  },
  // "include": ["**/*.ts"],
  // "exclude": ["node_modules","dist"]
  "include": ["src/**/*", "services/**/*"],
  "exclude": ["node_modules", "**/*.test.ts", "tests/**/*"]
}